{
    "manifest_version": 3,
    "name": "WaveHeaven Volume Control",
    "version": "1.0",
    "permissions": ["tabs", "activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
    }]
}